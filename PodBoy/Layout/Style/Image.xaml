<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:control="clr-namespace:PodBoy.Resource.Control"
                    xmlns:view="clr-namespace:PodBoy.View">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../Theme/ModernUITheme.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="VolumeBar" TargetType="{x:Type Rectangle}">
        <Setter Property="RadiusX" Value="0" />
        <Setter Property="RadiusY" Value="0" />
        <Setter Property="StrokeThickness" Value="1" />
        <Setter Property="StrokeLineJoin" Value="Round" />
        <Setter Property="StrokeStartLineCap" Value="Round" />
        <Setter Property="StrokeEndLineCap" Value="Round" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
    </Style>

    <Style x:Key="OverlayGridStyle" TargetType="Grid">
        <Setter Property="Visibility" Value="Collapsed" />
        <Setter Property="Background">
            <Setter.Value>
                <SolidColorBrush Color="White" Opacity="0.7" />
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="PodImageTemplate" TargetType="{x:Type ContentControl}">

        <Grid Margin="{TemplateBinding Padding}">
            <Border x:Name="Border" BorderBrush="White" BorderThickness="{TemplateBinding BorderThickness}"
                    Grid.Column="0" Grid.Row="0" Margin="0" CornerRadius="2"
                    VerticalAlignment="Center" HorizontalAlignment="Center">
                <Grid>
                    <Border x:Name="Mask" Background="White" CornerRadius="2" />
                    <Image x:Name="Image" Width="{Binding ImageProvider.DecodePixelWidth}"
                           Height="{Binding ImageProvider.DecodePixelWidth}"
                           Source="{Binding ImageProvider.Image}">
                        <Image.OpacityMask>
                            <VisualBrush Visual="{Binding ElementName=Mask}" />
                        </Image.OpacityMask>
                    </Image>
                </Grid>
            </Border>
            <!--<control:BusyWheel x:Name="BusyWheel" Width="20" Height="20"
                               Visibility="Collapsed" />-->
        </Grid>

        <ControlTemplate.Triggers>
            <!--<Trigger SourceName="Image" Property="Source" Value="{x:Null}">
                <Setter TargetName="BusyWheel" Property="Visibility" Value="Visible" />
                <Setter TargetName="Border" Property="Visibility" Value="Collapsed" />
            </Trigger>-->
            <DataTrigger Binding="{Binding ImageProvider.IsDefault}" Value="True">
                <Setter Property="BorderThickness" Value="0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>


    <DrawingImage x:Key="EmptyImage" />

    <ControlTemplate x:Key="EpisodePodImageTemplate" TargetType="{x:Type ContentControl}">
        <Grid Margin="{TemplateBinding Padding}">
            <Border x:Name="Border" BorderBrush="White" BorderThickness="{TemplateBinding BorderThickness}"
                    Grid.Column="0" Grid.Row="0" Margin="0" CornerRadius="2"
                    VerticalAlignment="Center" HorizontalAlignment="Center">
                <Grid>
                    <Border x:Name="Mask" Background="White" CornerRadius="2" />
                    <Image x:Name="Image" Width="{Binding ImageProvider.DecodePixelWidth}"
                           Height="{Binding ImageProvider.DecodePixelWidth}"
                           Source="{Binding ImageProvider.Image}">
                        <Image.OpacityMask>
                            <VisualBrush Visual="{Binding ElementName=Mask}" />
                        </Image.OpacityMask>
                    </Image>
                </Grid>
            </Border>

            <!--<control:BusyWheel x:Name="BusyWheel" Width="20" Height="20"
                               Visibility="Collapsed" />-->
        </Grid>
        <ControlTemplate.Triggers>
            <!--<Trigger SourceName="Image" Property="Source" Value="{x:Null}">
                <Setter TargetName="BusyWheel" Property="Visibility" Value="Visible" />
                <Setter TargetName="Border" Property="Visibility" Value="Collapsed" />
            </Trigger>-->
            <DataTrigger Binding="{Binding ImageProvider.IsDefault}" Value="True">
                <Setter Property="BorderThickness" Value="0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="PlaylistItemPodImageTemplate" TargetType="{x:Type ContentControl}">
        <Grid Margin="{TemplateBinding Padding}">
            <Border x:Name="Border" BorderBrush="White" BorderThickness="{TemplateBinding BorderThickness}"
                    Grid.Column="0" Grid.Row="0" Margin="0" CornerRadius="2"
                    VerticalAlignment="Center" HorizontalAlignment="Center">
                <Grid>
                    <Border x:Name="Mask" Background="White" CornerRadius="2" />
                    <Image x:Name="Image" Width="{Binding Item.ImageProvider.DecodePixelWidth}"
                           Height="{Binding Item.ImageProvider.DecodePixelWidth}"
                           Source="{Binding Item.ImageProvider.Image}">
                        <Image.OpacityMask>
                            <VisualBrush Visual="{Binding ElementName=Mask}" />
                        </Image.OpacityMask>
                    </Image>
                </Grid>
            </Border>

            <!--<control:BusyWheel x:Name="BusyWheel" Width="20" Height="20"
                               Visibility="Collapsed" />-->
        </Grid>
        <ControlTemplate.Triggers>
            <!--<Trigger SourceName="Image" Property="Source" Value="{x:Null}">
                <Setter TargetName="BusyWheel" Property="Visibility" Value="Visible" />
                <Setter TargetName="Border" Property="Visibility" Value="Collapsed" />
            </Trigger>-->
            <DataTrigger Binding="{Binding Item.ImageProvider.IsDefault}" Value="True">
                <Setter Property="BorderThickness" Value="0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="PlayIconAccentStroke" TargetType="{x:Type ToggleButton}">

        <Viewbox Stretch="Uniform" SnapsToDevicePixels="True">
            <Grid>
                <Canvas Name="PlayIcon" Width="50" Height="50" Background="Transparent">
                    <Canvas.Resources />
                    <!--Unknown tag: sodipodi:namedview-->
                    <!--Unknown tag: metadata-->
                    <Canvas Name="layer1">
                        <Canvas.RenderTransform>
                            <TranslateTransform X="-295.67619" Y="-395.25743" />
                        </Canvas.RenderTransform>
                        <Path Name="PlayIconSymbol" StrokeThickness="3" Stroke="{StaticResource DarkerGrayBrush}"
                              Fill="{StaticResource DarkerGrayBrush}" StrokeMiterLimit="4" StrokeLineJoin="Round"
                              StrokeStartLineCap="Round" StrokeEndLineCap="Round">
                            <Path.Data>
                                <PathGeometry Figures="m 300.67619 400.25742 0 40.00003 40 -20.00003 z"
                                              FillRule="NonZero" />
                            </Path.Data>
                        </Path>
                    </Canvas>
                </Canvas>

                <Canvas Name="PauseIcon" Width="50" Height="50" Visibility="Collapsed" Background="Transparent">
                    <Canvas.Resources />
                    <!--Unknown tag: sodipodi:namedview-->
                    <!--Unknown tag: metadata-->
                    <Canvas Name="PauseIconCanvas">
                        <Canvas.RenderTransform>
                            <TranslateTransform X="-295.67619" Y="-395.25743" />
                        </Canvas.RenderTransform>
                        <Canvas Name="g5021">
                            <Canvas.RenderTransform>
                                <TranslateTransform X="58.500004" Y="5.0000142" />
                            </Canvas.RenderTransform>
                            <Rectangle Canvas.Left="243" Canvas.Top="395" Width="15" Height="40" RadiusX="0"
                                       RadiusY="0" Name="PauseIconPathSymbol1"
                                       StrokeThickness="3" Stroke="{StaticResource DarkerGrayBrush}"
                                       StrokeMiterLimit="4"
                                       StrokeLineJoin="Round" StrokeStartLineCap="Round"
                                       StrokeEndLineCap="Round" />
                            <Rectangle Canvas.Left="266" Canvas.Top="395" Width="15" Height="40" RadiusX="0"
                                       RadiusY="0" Name="PauseIconPathSymbol2"
                                       StrokeThickness="3" Stroke="{StaticResource DarkerGrayBrush}"
                                       StrokeMiterLimit="4"
                                       StrokeLineJoin="Round" StrokeStartLineCap="Round"
                                       StrokeEndLineCap="Round" />
                        </Canvas>
                    </Canvas>
                </Canvas>

            </Grid>
        </Viewbox>

        <ControlTemplate.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="PlayIcon" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PauseIcon" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="True">
                <Setter TargetName="PauseIconPathSymbol1" Property="Fill" Value="{StaticResource DarkGrayBrush}" />
                <Setter TargetName="PauseIconPathSymbol2" Property="Fill" Value="{StaticResource DarkGrayBrush}" />
                <Setter TargetName="PauseIconPathSymbol1" Property="Stroke" Value="{StaticResource DarkGrayBrush}" />
                <Setter TargetName="PauseIconPathSymbol2" Property="Stroke" Value="{StaticResource DarkGrayBrush}" />

                <Setter TargetName="PlayIconSymbol" Property="Fill" Value="{StaticResource DarkGrayBrush}" />
                <Setter TargetName="PlayIconSymbol" Property="Stroke" Value="{StaticResource DarkGrayBrush}" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="PauseIconPathSymbol1" Property="Fill" Value="{StaticResource AccentColorBrush}" />
                <Setter TargetName="PauseIconPathSymbol2" Property="Fill" Value="{StaticResource AccentColorBrush}" />
                <Setter TargetName="PauseIconPathSymbol1" Property="Stroke" Value="{StaticResource AccentColorBrush}" />
                <Setter TargetName="PauseIconPathSymbol2" Property="Stroke" Value="{StaticResource AccentColorBrush}" />

                <Setter TargetName="PlayIconSymbol" Property="Fill" Value="{StaticResource AccentColorBrush}" />
                <Setter TargetName="PlayIconSymbol" Property="Stroke" Value="{StaticResource AccentColorBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

